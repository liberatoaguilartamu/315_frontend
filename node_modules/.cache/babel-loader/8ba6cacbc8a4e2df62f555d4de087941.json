{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// import HelloWorld from '../components/HelloWorld'\n\nexport default {\n  name: 'ClientCheckoutView',\n  components: {\n    //HelloWorld,\n  },\n  data: () => ({\n    sub_total: 0,\n    tax: 0,\n    toppings_array: [],\n    entrees_array: [],\n    justify: ['space-between']\n  }),\n  computed: {\n    calculate_tax() {\n      this.tax = this.sub_total * 0.00625;\n      return this.tax;\n    }\n  },\n  created() {\n    this.sub_total = this.$route.query.sub_total;\n    this.toppings_array = this.$route.query.toppings_array;\n    this.entrees_array = this.$route.query.entrees_array;\n    this.calculate_tax();\n  },\n  methods: {\n    move() {\n      this.$router.push({\n        path: 'client'\n      });\n    },\n    getData(data) {\n      alert(data);\n    }\n  }\n};","map":{"version":3,"mappings":";AA+IA;;AAEA;EACAA;EAEAC;IACA;EACA;EAEAC;IACAC;IACAC;IACAC;IACAC;IAEAC,UACA;EAEA;EAEAC;IACAC;MACA;MACA;IACA;EACA;EAEAC;IACA;IACA;IACA;IACA;EACA;EAEAC;IACAC;MACA;QACAC;MACA;IACA;IAEAC;MACAC;IACA;EACA;AAIA","names":["name","components","data","sub_total","tax","toppings_array","entrees_array","justify","computed","calculate_tax","created","methods","move","path","getData","alert"],"sourceRoot":"src/views","sources":["ClientCheckoutView.vue"],"sourcesContent":["<template>\n    <div>\n        <v-container class=\"Top\">\n            <v-row v-for=\"j in justify\" :key=\"j\" :justify=\"j\">\n                <v-col v-for=\"k in 1\" :key=\"k\">\n                    <br>\n                    <v-btn @click.prevent=\"move()\" small color=\"#4174D9\">Add more Items</v-btn>\n                </v-col>\n                <right>\n                    <v-col v-for=\"k in 1\" :key=\"k\">\n                        <!--ColorBlind mode button-->\n                        <v-switch v-model=\"singleExpand\" label=\"Colorblind Mode\"></v-switch>\n                    </v-col>\n                </right>\n            </v-row>\n        </v-container>\n\n        <!-- <v-img src=\"./pom.png\" width=\"300\"></v-img> -->\n        <v-container class=\"checkout\">\n            <v-row v-for=\"j in justify\" :key=\"j\" :justify=\"j\">\n                <!-- left -->\n                <v-col v-for=\"k in 1\" :key=\"k\">\n\n                    <br><br>\n                    <h1>Guest Information</h1>\n                    <br>\n                    <v-card class=\"pa-2\" width=\"600\">\n                        <v-row>\n                            <v-col cols=\"12\" sm=\"6\">\n                                <v-text-field v-model=\"message1\" label=\"First Name\" clearable></v-text-field>\n                            </v-col>\n                            <v-col cols=\"12\" sm=\"6\">\n                                <v-text-field v-model=\"message1\" label=\"Last Name\" clearable></v-text-field>\n                            </v-col>\n                            <v-col cols=\"12\" sm=\"6\">\n                                <v-text-field v-model=\"message1\" label=\"Email\" clearable></v-text-field>\n                            </v-col>\n                            <v-col cols=\"12\" sm=\"6\">\n                                <v-text-field v-model=\"message1\" label=\"Mobile Phone\" clearable></v-text-field>\n                            </v-col>\n                        </v-row>\n                    </v-card>\n                    <br><br>\n                    <h1>Payment Method</h1>\n                    <p1 class=\"payment\">Select a payment method below to continue with your order.</p1>\n                    <br><br>\n                    <v-card class=\"pa-2\" width=\"600\">\n                        <v-row>\n                            <v-col cols=\"12\" sm=\"6\">\n                                <v-text-field v-model=\"message1\" label=\"Credit Card Number\" clearable></v-text-field>\n                            </v-col>\n                            <v-col cols=\"12\" sm=\"6\">\n                                <v-text-field v-model=\"message1\" label=\"Exp. Date\" clearable></v-text-field>\n                            </v-col>\n                            <v-col cols=\"12\" sm=\"6\">\n                                <v-text-field v-model=\"message1\" label=\"CVV\" clearable></v-text-field>\n                            </v-col>\n                            <v-col cols=\"12\" sm=\"6\">\n                                <v-text-field v-model=\"message1\" label=\"Zip Code\" clearable></v-text-field>\n                            </v-col>\n                        </v-row>\n                    </v-card>\n\n\n\n                </v-col>\n                <!-- right -->\n                <v-col v-for=\"k in 1\" :key=\"k\">\n                    <br><br>\n                    <center>\n                        <h1>Your Order</h1>\n                    </center>\n                    <br>\n                    <v-card class=\"pa-10\">\n                        <p1 v-for=\"k in entrees_array\" :key=\"k\" class=\"Entree\">\n                            {{ k.name }}\n                            <br>\n                        </p1>\n                        <p1 v-for=\"k in toppings_array\" :key=\"k\" class=\"topping\">\n                            - {{ k.name }}\n                            <br>\n                        </p1>\n                    </v-card>\n                    <br><br>\n                    <v-card class=\"pa-10\" inlined tile>\n                        <v-row>\n                            <v-col>\n                                <p1 class='Entree'>\n                                    Subtotal:\n                                </p1>\n                            </v-col>\n                            <right>\n                                <v-col>\n                                    $ {{ sub_total }}\n                                </v-col>\n                            </right>\n                        </v-row>\n                        <v-row>\n                            <v-col>\n                                <p1 class='Entree'>\n                                    Tax:\n                                </p1>\n                            </v-col>\n                            <right>\n                                <v-col>\n                                    $ {{ tax.toFixed(2) }}\n                                </v-col>\n                            </right>\n                        </v-row>\n                        <v-row>\n                            <v-col>\n                                <p1 class='Entree'>\n                                    Order Total:\n                                </p1>\n                            </v-col>\n                        </v-row>\n                        <br>\n                        <center>\n                            <v-btn \n                            :loading=\"loading\"\n                            :disabled=\"loading\"\n                            @click=\"deleteAllItem('order'), insertOrderTable()\"\n                                color=\"#42a376\">Place Order</v-btn>\n                        </center>\n                    </v-card>\n\n                </v-col>\n            </v-row>\n        </v-container>\n\n\n        <!-- <v-btn @click.prevent=\"move()\" small color=\"#4174D9\">Add more Items</v-btn>\n        <h1>Welcome</h1>\n        {{ all_info }}\n        {{ sub_total }}\n        <br>\n        {{ toppings_array }}\n        <br>\n        {{ entrees_array }} -->\n    </div>\n</template>\n\n<script>\n// import HelloWorld from '../components/HelloWorld'\n\nexport default {\n    name: 'ClientCheckoutView',\n\n    components: {\n        //HelloWorld,\n    },\n\n    data: () => ({\n        sub_total: 0,\n        tax: 0,\n        toppings_array: [],\n        entrees_array: [],\n\n        justify: [\n            'space-between',\n        ],\n    }),\n\n    computed: {\n        calculate_tax() {\n            this.tax = this.sub_total * 0.00625;\n            return this.tax;\n        }\n    },\n\n    created() {\n        this.sub_total = this.$route.query.sub_total;\n        this.toppings_array = this.$route.query.toppings_array;\n        this.entrees_array = this.$route.query.entrees_array;\n        this.calculate_tax();\n    },\n\n    methods: {\n        move() {\n            this.$router.push({\n                path: 'client',\n            })\n        },\n\n        getData(data) {\n            alert(data)\n        },\n    }\n\n\n\n}\n</script>\n\n<style>\n.text-center {\n    margin-top: 13px;\n}\n\n.yourOrder {\n    font-weight: bold;\n    font-size: 20px;\n    align-items: left;\n}\n\n.Entree {\n    font-weight: bold;\n    font-size: 20px;\n    align-items: left;\n}\n\n.topping {\n    font-size: 13px;\n}\n\n.payment {\n    color: grey;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}