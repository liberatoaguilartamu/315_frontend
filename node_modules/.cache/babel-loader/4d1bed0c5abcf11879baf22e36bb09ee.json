{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// Clock import\nimport VueClock from 'vue-clock';\nexport default {\n  data: () => ({\n    dialog: false,\n    dialogDelete: false,\n    count: -1,\n    enable_toppings: false,\n    enable_entrees: true,\n    enalbe_mainProteins: false,\n    enalbe_subProteins: false,\n    tip: 0,\n    sub_total: 0,\n    total: 0,\n    tip1: false,\n    tip2: false,\n    tip3: false,\n    tip4: false,\n    light_mode: false,\n    dark_mode: true,\n    // Layout\n    justify: ['space-between'],\n    // Table\n    headers: [{\n      text: 'Item Number',\n      align: 'start',\n      sortable: false,\n      value: 'number'\n    }, {\n      text: 'Name',\n      value: 'name'\n    }, {\n      text: 'Price',\n      value: 'price'\n    }, {\n      text: 'Actions',\n      value: 'actions',\n      sortable: false\n    }],\n    items: [],\n    editedIndex: -1,\n    editedItem: {\n      number: 0,\n      name: '',\n      price: 0.0\n    },\n    defaultItem: {\n      number: 0,\n      name: '',\n      price: 0.0\n    },\n    mainEntrees_name: ['Gyros', 'Bowls'],\n    subEntrees_name: ['Hummus & Fita', 'Two Falafels', 'Drink'],\n    mainProteins_name: ['Chicken', 'Pork', 'Lamb', 'Beef'],\n    subProteins_name: ['Extra Chicken', 'Extra Pork', 'Extra Lamb', 'Extra Beef'],\n    toppings_name: ['Olive', 'Tomato', 'Lettuce', 'Red Onion', 'Cucumber', 'White Sauce', 'Brown Sauce', 'Extra White Sauce', 'Extra Brown Sauce']\n  }),\n  // data end\n\n  // computed\n  computed: {\n    formTitle() {\n      return this.editedIndex === -1 ? 'New Item' : 'Edit Item';\n    },\n    calculate_subTotal() {\n      let sum = 0;\n      for (let i = 0; i < this.items.length; i++) {\n        sum += this.items[i].price;\n      }\n      return this.sub_total = sum;\n    },\n    isDisabled() {\n      return this.enable_toppings;\n    },\n    isDisabled_mainProteins() {\n      return this.enalbe_mainProteins;\n    },\n    isDisabled_subProteins() {\n      return this.enalbe_subProteins;\n    },\n    isDisabled_entrees() {\n      return this.enable_entrees;\n    },\n    istip1() {\n      return this.tip1;\n    },\n    istip2() {\n      return this.tip2;\n    },\n    istip3() {\n      return this.tip3;\n    },\n    istip4() {\n      return this.tip4;\n    },\n    calculate_tip_1() {\n      let sum = 0;\n      for (let i = 0; i < this.items.length; i++) {\n        sum += this.items[i].price;\n      }\n      this.tip = sum * 0.05;\n      return this.tip;\n    },\n    calculate_tip_2() {\n      let sum = 0;\n      for (let i = 0; i < this.items.length; i++) {\n        sum += this.items[i].price;\n      }\n      this.tip = sum * 0.10;\n      return this.tip;\n    },\n    calculate_tip_3() {\n      let sum = 0;\n      for (let i = 0; i < this.items.length; i++) {\n        sum += this.items[i].price;\n      }\n      this.tip = sum * 0.15;\n      return this.tip;\n    },\n    calculate_tip_4() {\n      let sum = 0;\n      for (let i = 0; i < this.items.length; i++) {\n        sum += this.items[i].price;\n      }\n      this.tip = sum * 0.20;\n      return this.tip;\n    },\n    calculate_total() {\n      let sum = 0;\n      for (let i = 0; i < this.items.length; i++) {\n        sum += this.items[i].price;\n      }\n      return (sum + this.tip).toFixed(2);\n    }\n  },\n  // watch\n  watch: {\n    dialog(val) {\n      val || this.close();\n    },\n    dialogDelete(val) {\n      val || this.closeDelete();\n    }\n  },\n  // Clock\n  components: {\n    VueClock\n  },\n  // methods\n  methods: {\n    // Entrees buttons\n    click_entrees: function (e) {\n      if (e === 'Gyros') {\n        this.count++;\n        this.items.push({\n          number: this.count,\n          name: 'Gyros',\n          price: 8.09\n        });\n      } else if (e === 'Bowls') {\n        this.count++;\n        this.items.push({\n          number: this.count,\n          name: 'Bowls',\n          price: 8.09\n        });\n      } else if (e === 'Hummus & Fita') {\n        this.count++;\n        this.items.push({\n          number: this.count,\n          name: 'Hummus & Fita',\n          price: 3.49\n        });\n      } else if (e === 'Two Falafels') {\n        this.count++;\n        this.items.push({\n          number: this.count,\n          name: 'Two Falafels',\n          price: 3.59\n        });\n      } else if (e === 'Drink') {\n        this.count++;\n        this.items.push({\n          number: this.count,\n          name: 'Drink',\n          price: 2.45\n        });\n      }\n    },\n    // Proteins buttons\n    click_proteins: function (e) {\n      if (e === 'Chicken') {\n        this.items.push({\n          number: this.count,\n          name: 'Chicken',\n          price: 0\n        });\n      } else if (e === 'Pork') {\n        this.items.push({\n          number: this.count,\n          name: 'Pork',\n          price: 0\n        });\n      } else if (e === 'Lamb') {\n        this.items.push({\n          number: this.count,\n          name: 'Lamb',\n          price: 0\n        });\n      } else if (e === 'Beef') {\n        this.items.push({\n          number: this.count,\n          name: 'Beef',\n          price: 0\n        });\n      } else if (e === 'Extra Chicken') {\n        this.items.push({\n          number: this.count,\n          name: 'Extra Pork',\n          price: 1.99\n        });\n      } else if (e === 'Extra Pork') {\n        this.items.push({\n          number: this.count,\n          name: 'Extra Pork',\n          price: 1.99\n        });\n      } else if (e === 'Extra Lamb') {\n        this.items.push({\n          number: this.count,\n          name: 'Extra Lamb',\n          price: 1.99\n        });\n      } else if (e === 'Extra Beef') {\n        this.items.push({\n          number: this.count,\n          name: 'Extra Beef',\n          price: 1.99\n        });\n      }\n    },\n    // Toppings buttons\n    click_toppings: function (e) {\n      if (e === 'Olive') {\n        this.items.push({\n          number: this.count,\n          name: 'Olive',\n          price: 0\n        });\n      } else if (e === 'Tomato') {\n        this.items.push({\n          number: this.count,\n          name: 'Tomato',\n          price: 0\n        });\n      } else if (e === 'Lettuce') {\n        this.items.push({\n          number: this.count,\n          name: 'Lettuce',\n          price: 0\n        });\n      } else if (e === 'Red Onion') {\n        this.items.push({\n          number: this.count,\n          name: 'Red Onion',\n          price: 0\n        });\n      } else if (e === 'Cucumber') {\n        this.items.push({\n          number: this.count,\n          name: 'Cucumber',\n          price: 0\n        });\n      } else if (e === 'White Sauce') {\n        this.items.push({\n          number: this.count,\n          name: 'White Sauce',\n          price: 0\n        });\n      } else if (e === 'Brown Sauce') {\n        this.items.push({\n          number: this.count,\n          name: 'Brwon Sauce',\n          price: 0\n        });\n      } else if (e === 'Extra White Sauce') {\n        this.items.push({\n          number: this.count,\n          name: 'Extra White Sauce',\n          price: 0.39\n        });\n      } else if (e === 'Extra Brown Sauce') {\n        this.items.push({\n          number: this.count,\n          name: 'Extra Brown Sauce',\n          price: 0.39\n        });\n      }\n    },\n    tip_control(k) {\n      if (k === 1) {\n        this.tip1 = true;\n        this.tip2 = false;\n        this.tip3 = false;\n        this.tip4 = false;\n      } else if (k === 2) {\n        this.tip1 = false;\n        this.tip2 = true;\n        this.tip3 = false;\n        this.tip4 = false;\n      } else if (k === 3) {\n        this.tip1 = false;\n        this.tip2 = false;\n        this.tip3 = true;\n        this.tip4 = false;\n      } else if (k === 4) {\n        this.tip1 = false;\n        this.tip2 = false;\n        this.tip3 = false;\n        this.tip4 = true;\n      }\n    },\n    enable_all_buttons(k) {\n      if (k === 'Gyros' || k === 'Bowls') {\n        this.enable_toppings = true;\n        this.enalbe_mainProteins = true;\n        this.enalbe_subProteins = false;\n      }\n    },\n    enable_entree_buttons(k) {\n      this.enable_entrees = false;\n      if (k === 1) {\n        this.enable_entrees = true;\n      }\n    },\n    disable_mainProteins_buttons(k) {\n      this.enalbe_mainProteins = false;\n    },\n    enalbe_subproteins_buttons() {\n      this.enalbe_subProteins = true;\n    },\n    disable_subProteins_buttons() {\n      this.enalbe_subProteins = false;\n    },\n    addItem() {\n      this.enable_toppings = false;\n      this.enable_entrees = true;\n      this.enalbe_mainProteins = false;\n      this.enalbe_subProteins = false;\n    },\n    deleteItem(item) {\n      this.editedIndex = this.items.indexOf(item);\n      this.editedItem = Object.assign({}, item);\n      this.dialogDelete = true;\n      if (item.name === 'Gyros' || item.name === 'Bowls' || item.name === 'Hummus & Fita' || item.name === 'Two Falafels' || item.name === 'Drink') {\n        this.count -= 1;\n        this.enable_entrees = true;\n        this.enalbe_mainProteins = false;\n        this.enalbe_subProteins = false;\n        this.enable_toppings = false;\n      }\n      if (item.name === 'Chicken' || item.name === 'Pork' || item.name === 'Lamb' || item.name === 'Beef') {\n        this.enalbe_mainProteins = true;\n        this.enalbe_subProteins = false;\n      }\n      if (item.name === 'Extra Chicken' || item.name === 'Extra Pork' || item.name === 'Extra Lamb' || item.name === 'Extra Beef') {\n        this.enalbe_subProteins = true;\n      }\n    },\n    deleteAllItem(e) {\n      if (e == 'clear') {\n        for (let i = this.items.length; i >= 0; i--) {\n          this.items.pop(i);\n        }\n        this.count = -1;\n        this.enalbe_mainProteins = false;\n        this.enalbe_subProteins = false;\n        this.enable_toppings = false;\n        this.enable_entrees = true;\n        this.tip = 0;\n        this.total = 0;\n        this.sub_total = 0;\n        this.tip1 = false;\n        this.tip2 = false;\n        this.tip3 = false;\n        this.tip4 = false;\n        alert(\"Cleared!\");\n      } else {\n        for (let i = this.items.length; i >= 0; i--) {\n          this.items.pop(i);\n        }\n        this.count = -1;\n        this.enalbe_mainProteins = false;\n        this.enalbe_subProteins = false;\n        this.enable_toppings = false;\n        this.enable_entrees = true;\n        this.tip = 0;\n        this.total = 0;\n        this.sub_total = 0;\n        this.tip1 = false;\n        this.tip2 = false;\n        this.tip3 = false;\n        this.tip4 = false;\n      }\n    },\n    deleteItemConfirm() {\n      this.items.splice(this.editedIndex, 1);\n      this.closeDelete();\n    },\n    close() {\n      this.dialog = false;\n      this.$nextTick(() => {\n        this.editedItem = Object.assign({}, this.defaultItem);\n        this.editedIndex = -1;\n      });\n    },\n    closeDelete() {\n      this.dialogDelete = false;\n      this.$nextTick(() => {\n        this.editedItem = Object.assign({}, this.defaultItem);\n        this.editedIndex = -1;\n      });\n    },\n    save() {\n      if (this.editedIndex > -1) {\n        Object.assign(this.items[this.editedIndex], this.editedItem);\n      } else {\n        this.items.push(this.editedItem);\n      }\n      this.close();\n    },\n    getData(data) {\n      alert(data);\n    },\n    light_mode_button() {\n      this.$vuetify.theme.dark = false;\n      this.dark_mode = false;\n    },\n    light_mode_button_outlined() {\n      this.$vuetify.theme.dark = false;\n      this.light_mode = true;\n      this.dark_mode = false;\n    },\n    dark_mode_button() {\n      this.$vuetify.theme.dark = true;\n      this.light_mode = false;\n    },\n    dark_mode_button_outlined() {\n      this.$vuetify.theme.dark = true;\n      this.light_mode = false;\n      this.dark_mode = true;\n    }\n  }\n};","map":{"version":3,"mappings":";AA6QA;AACA;AAEA;EACAA;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IAEA;IACAC,UACA,gBACA;IAEA;IACAC,UACA;MACAC;MACAC;MACAC;MACAC;IACA,GACA;MAAAH;MAAAG;IAAA,GACA;MAAAH;MAAAG;IAAA,GACA;MAAAH;MAAAG;MAAAD;IAAA,EACA;IAEAE;IAEAC;IAEAC;MACAC;MACAC;MACAC;IACA;IAEAC;MACAH;MACAC;MACAC;IACA;IAEAE;IACAC;IACAC;IACAC;IACAC;EAEA;EAAA;;EAEA;EACAC;IACAC;MACA;IACA;IAEAC;MACA;MACA;QACAC;MACA;MACA;IACA;IAEAC;MACA;IACA;IAEAC;MACA;IACA;IAEAC;MACA;IACA;IAEAC;MACA;IACA;IAEAC;MACA;IACA;IAEAC;MACA;IACA;IAEAC;MACA;IACA;IAEAC;MACA;IACA;IAEAC;MACA;MACA;QACAT;MACA;MACA;MACA;IACA;IAEAU;MACA;MACA;QACAV;MACA;MACA;MACA;IACA;IAEAW;MACA;MACA;QACAX;MACA;MACA;MACA;IACA;IAEAY;MACA;MACA;QACAZ;MACA;MACA;MACA;IACA;IAEAa;MACA;MACA;QACAb;MACA;MAEA;IACA;EACA;EAEA;EACAc;IACAnD;MACAoD;IACA;IACAnD;MACAmD;IACA;EACA;EAEA;EACAC;IAAAC;EAAA;EAEA;EACAC;IACA;IACAC;MACA;QACA;QACA;UACA/B;UAAAC;UAAAC;QACA;MACA,OACA;QACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA,OACA;QACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA,OACA;QACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA,OACA;QACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA;IACA;IAEA;IACA8B;MACA;QACA;UACAhC;UAAAC;UAAAC;QACA;MACA,OACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA,OACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA,OACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA,OACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA,OACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA,OACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA,OACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA;IACA;IAEA;IACA+B;MACA;QACA;UACAjC;UAAAC;UAAAC;QACA;MACA,OACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA,OACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA,OACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA,OACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA,OACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA,OACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA,OACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA,OACA;QACA;UACAF;UAAAC;UAAAC;QACA;MACA;IACA;IAEAgC;MACA;QACA;QACA;QACA;QACA;MACA,OACA;QACA;QACA;QACA;QACA;MACA,OACA;QACA;QACA;QACA;QACA;MACA,OACA;QACA;QACA;QACA;QACA;MACA;IACA;IAEAC;MACA;QACA